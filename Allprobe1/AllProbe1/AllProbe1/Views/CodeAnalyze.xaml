<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AllProbe1.Views.CodeAnalyze"
             BackgroundColor="White">
    <Grid RowSpacing="0">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>

        <ContentView Grid.Row="0"
                     BackgroundColor="#dadada">
            <Label x:Name="lblURL" LineBreakMode="TailTruncation"
               HorizontalTextAlignment="Center" VerticalTextAlignment="Center" Margin="3,3,3,3"
               BackgroundColor="Transparent" TextColor="#ff00bfff" FontSize="Medium"/>
        </ContentView>
        <Label Grid.Row="1"
               Text="Feature is not available yet.&#x0a;&#x0a;Please check in future releases!"
               HorizontalTextAlignment="Center" VerticalTextAlignment="Center"/>
    </Grid>
</ContentPage>